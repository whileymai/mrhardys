#header.engoc_header_s1 {
  #topbar {
    background: $tb_bg;
    
    .topbar-left {
      .topbar-title {
        border-right: 1px solid $tb_border;
        color: $tb_text;
        
        a {
          color: $tb_link;
        }
      }
      a {
        color: $tb_action;
        border-right: 1px solid $tb_border;
      }
    }
    
    .social a {
      i {
        color: $tb_social;
      }

      &:hover, &:focus {
        i {
          color: $tb_social_hover;
        }
      }
    }
  }
  
  .header-top {
    background-color: $nav_bg_wp;
    
    .container {
      background-color: $nav_bg;

      &:before {
        background: $nav_border;
      }
      
      .mega-menu ul.navbar-nav li a {
        color: $nav_link;
      }
      
      .mega-menu ul.navbar-nav ul.menu-level-1 {
        background: $subnav_bg;
        
        li a {
          color: $subnav_link;
          border-bottom: 1px solid $subnav_border;

          &:hover, &:focus {
            color: $subnav_link_hover;
          }
        }
      }

      .mega-menu ul.navbar-nav {
        ul.menu-level-1 li:last-child a {
          border: none;
        }

        .sub-menu {
          background: $subnav_bg;

          ul.menu-level-1 {

            ul.menu-level-2 {
              li {
                background: $subnav_bg;
              }
              
              &:after {
                background-color: $subnav_border;
              }
            }

            li.level2 a {
              border: none;
            }

            li.level3 a {
              color: $subnav_link_mega;

              &:hover, &:focus {
                color: $subnav_link_hover;
              }
            }
          }
        }
      }
      
      .cart i, .search i {
        color: $nav_action;
      }
      
      .cart .cart-count {
        color: $nav_action_number;
      }
    }
  }
}

@media screen and (max-width: 480px) {
  #header.engoc_header_s1 #topbar {
    padding: 0px;
    height: 30px;
    
    .topbar-left {
      .topbar-title {
        border: none;
        float: left;
        width: 66%;

        a {
          border: none;
          float: left;
        }
        
        span {
          display: block;
        }
      }

      a {
        border-left: 1px solid $tb_border;
        border-right: 0px solid $tb_border;
        width: 18%;
        float: right;

        &:nth-child(3) {
          border-left: 1px solid $tb_border;
          border-right: 0px solid $tb_border;
        }
        
        &:nth-child(2) {
          margin-right: -4%;
        }
      }
    }
  }
}

