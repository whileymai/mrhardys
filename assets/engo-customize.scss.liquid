/* Css Sticky Menu */

.engoc_fixed {
  left:0;
  max-width:100%;
  overflow:visible;
  position:fixed !important;
  top:0;
  width:100%;
  z-index:1000;
  background-color: #fff;
  box-shadow: 0 1px 10px rgba(0,0,0,0.3);
}

/* About Us page */
.engoc_title_service {
  font: 700 14px Oswald;
  letter-spacing: 3px;
  text-transform: uppercase;
  text-align: center;
  padding-bottom: 15px;
  margin-bottom: 15px;
  margin-top: 30px;
  position: relative;

  &:after {
    position: absolute;
    content: "";
    left: 50%;
    bottom: 0px;
    height: 1px;
    background: #000;
    width: 30px;
    margin-left: -15px;
  }
}

@media (min-width: 768px) {
  .engoc_60 {
    margin-top: 60px;
  }

  .engoc_title_service {
    margin-top: 80px;
  }

  .engoc_iconbox_service {
    margin-bottom: 40px;
  }
}

/* Contact Us page */
.engoc_heightmap_contact {
  height: 600px;
}

