<div data-section-id="{{ section.id }}" data-section-type="section-slider-v1" style="{% if section.settings.margin_top != blank %}margin-top: {{section.settings.margin_top}}px;{%endif%} {% if section.settings.margin_bottom != blank %}margin-bottom: {{section.settings.margin_bottom}}px;{% endif %}">
  <div class="container">
    <div class="tp-banner-container nav-hidden dot-home1 ver1">
      <div class="tp-banner" >
        <ul> 
          {% if section.blocks.size > 0 %}
          {% for block in section.blocks %}
          
            {% if block.type == 'image' %}   
            <li data-transition="{{block.settings.slider_transition}}" data-slotamount="6" data-masterspeed="1000" >
              <!-- MAIN IMAGE -->
              <img src="{% if block.settings.slider_bg == blank %}{% capture _img_url %}slider_v1_bg.jpg{% endcapture %}{{ _img_url | asset_url}}{% else %}{{ block.settings.slider_bg | img_url: "master" }}{% endif %}"  alt="background"  data-bgfit="cover" data-bgposition="left top" data-bgrepeat="no-repeat">

              <!-- LAYER NR. 5 -->
              <div class="tp-caption {{block.settings.slider_img_effect}} customout"
                   data-x="{{block.settings.slider_img_x}}" data-hoffset="-140"
                   data-y="{{block.settings.slider_img_y}}" data-voffset="-103"
                   data-customin="x:0;y:0;z:0;rotationX:0;rotationY:0;rotationZ:0;scaleX:5;scaleY:5;skewX:0;skewY:0;opacity:0;transformPerspective:600;transformOrigin:50% 50%;"
                   data-customout="x:0;y:0;z:0;rotationX:0;rotationY:0;rotationZ:0;scaleX:0.75;scaleY:0.75;skewX:0;skewY:0;opacity:0;transformPerspective:600;transformOrigin:50% 50%;"
                   data-speed="1000"
                   data-start="1500"
                   data-easing="Power4.easeOut"
                   data-endspeed="500"
                   data-endeasing="Power4.easeIn"
                   data-captionhidden="on"
                   style="z-index: 7"><img src="{% if block.settings.slider_img == blank %}{% capture _img_url %}slider_v1_img.jpg{% endcapture %}{{ _img_url | asset_url}}{% else %}{{ block.settings.slider_img | img_url: "master" }}{% endif %}" alt="image">
              </div>

              <!-- LAYER NR. 3 -->
              <div class="tp-caption {{block.settings.slider_content1_textEffect}} font-cap color-black font-play letter-spacing-5 font-400 customout"
                   data-x="{{block.settings.slider_content1_x}}"
                   data-y="{{block.settings.slider_content1_y}}"
                   data-customout="x:0;y:0;z:0;rotationX:0;rotationY:0;rotationZ:0;scaleX:0;scaleY:0.75;skewX:0;skewY:0;opacity:0;transformPerspective:600;transformOrigin:50% 50%;"
                   data-speed="500"
                   data-start="2500"   
                   data-easing="Power4.easeOut"
                   data-endspeed="300"
                   data-endeasing="Power1.easeIn"
                   data-captionhidden="on"
                   style="z-index: 8"><p class="size-50">{{block.settings.slider_content1_1}} <br>{{block.settings.slider_content1_2}}</p>

              </div>

              <!-- LAYER NR. 3 -->
              <div class="tp-caption {{block.settings.slider_content2_textEffect}} color-grays  letter-spacing-5 customout size-14 "
                   data-x="{{block.settings.slider_content2_x}}"
                   data-y="{{block.settings.slider_content2_y}}"
                   data-customout="x:0;y:0;z:0;rotationX:0;rotationY:0;rotationZ:0;scaleX:0;scaleY:0.75;skewX:0;skewY:0;opacity:0;transformPerspective:600;transformOrigin:50% 50%;"
                   data-speed="500"
                   data-start="2700"
                   data-easing="Power4.easeOut"
                   data-endspeed="300"
                   data-endeasing="Power1.easeIn"
                   data-captionhidden="on"
                   style="z-index: 9"><span class="size-14 font-os weight-100">{{block.settings.slider_content2}}</span>
              </div>

              <!-- LAYER NR. 3 -->
              <div class="tp-caption {{block.settings.slider_icon_effect}} customout"
                   data-x="{{block.settings.slider_icon_x}}"
                   data-y="{{block.settings.slider_icon_y}}"
                   data-customout="x:0;y:0;z:0;rotationX:0;rotationY:0;rotationZ:0;scaleX:0;scaleY:0.75;skewX:0;skewY:0;opacity:0;transformPerspective:600;transformOrigin:50% 50%;"
                   data-speed="500"
                   data-start="2900"
                   data-easing="Power4.easeOut"
                   data-endspeed="300"
                   data-endeasing="Power1.easeIn"
                   data-captionhidden="on"
                   style="z-index: 9"><p class="border"><img src="{% if block.settings.slider_icon == blank %}{% capture _img_url %}slider_v1_line.jpg{% endcapture %}{{ _img_url | asset_url}}{% else %}{{ block.settings.slider_icon | img_url: "master" }}{% endif %}" alt="icon"></p>
              </div>

              <!-- LAYER NR. 3 -->
              <div class="tp-caption {{block.settings.slider_content3_textEffect}} t-transform-n color-5f5f5f font-400 font-pop customout size-14 lh-23"
                   data-x="{{block.settings.slider_content3_x}}"
                   data-y="{{block.settings.slider_content3_y}}"
                   data-customout="x:0;y:0;z:0;rotationX:0;rotationY:0;rotationZ:0;scaleX:0;scaleY:0.75;skewX:0;skewY:0;opacity:0;transformPerspective:600;transformOrigin:50% 50%;"
                   data-speed="500"
                   data-start="3100"
                   data-easing="Power4.easeOut"
                   data-endspeed="300"
                   data-endeasing="Power1.easeIn"
                   data-captionhidden="on"
                   style="z-index: 10"><p class="size-14">{{block.settings.slider_content3_1}}<br>{{block.settings.slider_content3_2}}<br>{{block.settings.slider_content3_3}}</p>
              </div>
              <!-- LAYER NR. 5 -->
              <div class="tp-caption {{block.settings.slider_btn_effect}} customout font-os link-v4"
                   data-x="{{block.settings.slider_btn_x}}"
                   data-y="{{block.settings.slider_btn_y}}"
                   data-customout="x:0;y:0;z:0;rotationX:0;rotationY:0;rotationZ:0;scaleX:0;scaleY:0.75;skewX:0;skewY:0;opacity:0;transformPerspective:600;transformOrigin:50% 50%;"
                   data-speed="500"
                   data-start="3300"
                   data-easing="Power4.easeOut"
                   data-endspeed="300"
                   data-endeasing="Power1.easeIn"
                   data-captionhidden="on"
                   style="z-index: 9"><a class="hover-black link-v5" href="{{block.settings.slider_btn_url}}" title="{{block.settings.slider_btn_name}}">{{block.settings.slider_btn_name}}<i class="link-icon-black"></i></a>
              </div>
            </li>
            {% endif %}
          
          	{% if block.type == 'video' %}   
            <li data-transition="{{block.settings.slider_transition}}" data-slotamount="6" data-masterspeed="1000" >
			  {% if block.settings.choose_videobg =="vimeo" %}
              <!-- BACKGROUND VIMEO VIDEO LAYER -->
              <div class="rs-background-video-layer" 
                   data-forcerewind="on" 
                   data-volume="mute" 
                   data-vimeoid="{{block.settings.slider_video_id}}" 
                   data-videoattributes="title=0&amp;byline=0&amp;portrait=0&amp;api=1" 
                   data-videowidth="100%" 
                   data-videoheight="100%" 
                   data-videocontrols="none"
                   data-videoloop="loop" 
                   data-forceCover="1" 
                   data-aspectratio="4:3" 
                   data-autoplay="true" 
                   data-autoplayonlyfirsttime="false" 
                   data-nextslideatend="true" 
                   >
              </div>
              {% endif %}
              
              {% if block.settings.choose_videobg =="youtube" %}
              <!-- BACKGROUND YOUTUBE VIDEO LAYER -->
              <div class="rs-background-video-layer" 
                   data-forcerewind="on" 
                   data-volume="mute" 
                   data-ytid="{{block.settings.slider_video_id}}" 
                   data-videoattributes="version=3&amp;enablejsapi=1&amp;html5=1&amp;hd=1&amp;wmode=opaque&amp;showinfo=0&amp;ref=0&amp;autoplay=1;origin=https://www.youtube.com/;" 
                   data-videorate="1" 
                   data-videowidth="100%" 
                   data-videoheight="100%" 
                   data-videocontrols="none" 
                   data-videoloop="loop" 
                   data-forceCover="1" 
                   data-aspectratio="4:3" 
                   data-autoplay="true" 
                   data-autoplayonlyfirsttime="false" 
                   data-nextslideatend="true" 
                   >
              </div>
              {% endif %}

              <!-- LAYER NR. 3 -->
              <div class="tp-caption {{block.settings.slider_video_content1_textEffect}} font-cap color-black font-play letter-spacing-5 font-400 customout"
                   data-x="{{block.settings.slider_video_content1_x}}"
                   data-y="{{block.settings.slider_video_content1_y}}"
                   data-customout="x:0;y:0;z:0;rotationX:0;rotationY:0;rotationZ:0;scaleX:0;scaleY:0.75;skewX:0;skewY:0;opacity:0;transformPerspective:600;transformOrigin:50% 50%;"
                   data-speed="500"
                   data-start="2500"   
                   data-easing="Power4.easeOut"
                   data-endspeed="300"
                   data-endeasing="Power1.easeIn"
                   data-captionhidden="on"
                   style="z-index: 8"><p class="size-50">{{block.settings.slider_video_content1_1}} <br>{{block.settings.slider_video_content1_2}}</p>

              </div>

              <!-- LAYER NR. 3 -->
              <div class="tp-caption {{block.settings.slider_video_content2_textEffect}} color-grays  letter-spacing-5 customout size-14 "
                   data-x="{{block.settings.slider_video_content2_x}}"
                   data-y="{{block.settings.slider_video_content2_y}}"
                   data-customout="x:0;y:0;z:0;rotationX:0;rotationY:0;rotationZ:0;scaleX:0;scaleY:0.75;skewX:0;skewY:0;opacity:0;transformPerspective:600;transformOrigin:50% 50%;"
                   data-speed="500"
                   data-start="2700"
                   data-easing="Power4.easeOut"
                   data-endspeed="300"
                   data-endeasing="Power1.easeIn"
                   data-captionhidden="on"
                   style="z-index: 9"><span class="size-14 font-os weight-100">{{block.settings.slider_video_content2}}</span>
              </div>

              <!-- LAYER NR. 3 -->
              <div class="tp-caption {{block.settings.slider_video_icon_effect}} customout"
                   data-x="{{block.settings.slider_video_icon_x}}"
                   data-y="{{block.settings.slider_video_icon_y}}"
                   data-customout="x:0;y:0;z:0;rotationX:0;rotationY:0;rotationZ:0;scaleX:0;scaleY:0.75;skewX:0;skewY:0;opacity:0;transformPerspective:600;transformOrigin:50% 50%;"
                   data-speed="500"
                   data-start="2900"
                   data-easing="Power4.easeOut"
                   data-endspeed="300"
                   data-endeasing="Power1.easeIn"
                   data-captionhidden="on"
                   style="z-index: 9"><p class="border"><img src="{% if block.settings.slider_video_icon == blank %}{% capture _img_url %}slider_v1_line.jpg{% endcapture %}{{ _img_url | asset_url}}{% else %}{{ block.settings.slider_video_icon | img_url: "master" }}{% endif %}" alt="icon"></p>
              </div>

              <!-- LAYER NR. 3 -->
              <div class="tp-caption {{block.settings.slider_video_content3_textEffect}} t-transform-n color-5f5f5f font-400 font-pop customout size-14 lh-23"
                   data-x="{{block.settings.slider_video_content3_x}}"
                   data-y="{{block.settings.slider_video_content3_y}}"
                   data-customout="x:0;y:0;z:0;rotationX:0;rotationY:0;rotationZ:0;scaleX:0;scaleY:0.75;skewX:0;skewY:0;opacity:0;transformPerspective:600;transformOrigin:50% 50%;"
                   data-speed="500"
                   data-start="3100"
                   data-easing="Power4.easeOut"
                   data-endspeed="300"
                   data-endeasing="Power1.easeIn"
                   data-captionhidden="on"
                   style="z-index: 10"><p class="size-14">{{block.settings.slider_video_content3_1}}<br>{{block.settings.slider_video_content3_2}}<br>{{block.settings.slider_video_content3_3}}</p>
              </div>
              <!-- LAYER NR. 5 -->
              <div class="tp-caption {{block.settings.slider_video_btn_effect}} customout font-os link-v4"
                   data-x="{{block.settings.slider_video_btn_x}}"
                   data-y="{{block.settings.slider_video_btn_y}}"
                   data-customout="x:0;y:0;z:0;rotationX:0;rotationY:0;rotationZ:0;scaleX:0;scaleY:0.75;skewX:0;skewY:0;opacity:0;transformPerspective:600;transformOrigin:50% 50%;"
                   data-speed="500"
                   data-start="3300"
                   data-easing="Power4.easeOut"
                   data-endspeed="300"
                   data-endeasing="Power1.easeIn"
                   data-captionhidden="on"
                   style="z-index: 9"><a class="hover-black link-v5" href="{{block.settings.slider_video_btn_url}}" title="{{block.settings.slider_btn_name}}">{{block.settings.slider_video_btn_name}}<i class="link-icon-black"></i></a>
              </div>
            </li>
            {% endif %}
          
          {% endfor %}
          {% endif %}
        </ul>
        <div class="tp-bannertimer"></div>
      </div>
    </div>
  </div>
</div>

<script>
  /* All setting bullet: https://www.themepunch.com/revsliderjquery-doc/navigation/#navbullets */
  jQuery(document).ready(function($) {
    if ($(".tp-banner").length) {
      $('.ver1 .tp-banner').revolution({
        jsFileLocation:"",
        delay: {{section.settings.slideshow_time}},
        startwidth: {{section.settings.slider_startwidth}},
        startheight: {{section.settings.slider_startheight}},
        hideThumbs: 10,
        fullWidth: "on",
        forceFullWidth: "on",
        navigation: { 
          touch:{
           touchenabled:"on",
           swipe_treshold : 75,
           swipe_min_touches : 1,
           drag_block_vertical:false,
           swipe_direction:"horizontal"
          },
          bullets:{
          style:"simplebullets round hidebullets",
          enable:true,
          container:"slider",
          rtl:false,
          hide_onmobile:false,
          hide_onleave:true,
          hide_delay:200,
          hide_delay_mobile:1200,
          hide_under:0,
          hide_over:9999,
          tmp:'', 
          direction:"vertical",
          space:0,       
          h_align:"left",
          v_align:"center",
          h_offset:0,
          v_offset:20
          }
        }
      });
      $(".tp-leftarrow").html("");
      $(".tp-rightarrow").html("");
    }
  });
</script>

